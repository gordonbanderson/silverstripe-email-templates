{
    "name": "lekoala/silverstripe-email-templates",
    "description": "Adds email-templates in the SilverStripe CMS",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "email-templates",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [{
        "name": "LeKoala",
        "email": "thomas@lekoala.be"
    }],
    "require": {
        "php" : "~5.6|~7.0",
        "silverstripe/framework": "^4.1",
        "silverware/calendar": "~1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\EmailTemplates\\": "code/",
            "LeKoala\\EmailTemplates\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "email-templates",
        "expose": [
            "images",
            "css",
            "javascript",
            "thirdparty"
        ]
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-email-templates/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
